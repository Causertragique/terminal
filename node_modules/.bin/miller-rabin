#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/guillaumehetu/Library/Mobile Documents/com~apple~CloudDocs/Terminal Command Reference Guide/node_modules/miller-rabin/bin/node_modules:/Users/guillaumehetu/Library/Mobile Documents/com~apple~CloudDocs/Terminal Command Reference Guide/node_modules/miller-rabin/node_modules:/Users/guillaumehetu/Library/Mobile Documents/com~apple~CloudDocs/Terminal Command Reference Guide/node_modules:/Users/guillaumehetu/Library/Mobile Documents/com~apple~CloudDocs/node_modules:/Users/guillaumehetu/Library/Mobile Documents/node_modules:/Users/guillaumehetu/Library/node_modules:/Users/guillaumehetu/node_modules:/Users/node_modules:/node_modules:/Users/guillaumehetu/Library/Mobile Documents/com~apple~CloudDocs/Terminal Command Reference Guide/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/guillaumehetu/Library/Mobile Documents/com~apple~CloudDocs/Terminal Command Reference Guide/node_modules/miller-rabin/bin/node_modules:/Users/guillaumehetu/Library/Mobile Documents/com~apple~CloudDocs/Terminal Command Reference Guide/node_modules/miller-rabin/node_modules:/Users/guillaumehetu/Library/Mobile Documents/com~apple~CloudDocs/Terminal Command Reference Guide/node_modules:/Users/guillaumehetu/Library/Mobile Documents/com~apple~CloudDocs/node_modules:/Users/guillaumehetu/Library/Mobile Documents/node_modules:/Users/guillaumehetu/Library/node_modules:/Users/guillaumehetu/node_modules:/Users/node_modules:/node_modules:/Users/guillaumehetu/Library/Mobile Documents/com~apple~CloudDocs/Terminal Command Reference Guide/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../miller-rabin/bin/miller-rabin" "$@"
else
  exec node  "$basedir/../miller-rabin/bin/miller-rabin" "$@"
fi
